#include<stdio.h>
void Multiply(int );

int main()
{
    int n;
    scanf("%d",&n);
    Multiply(n);

}

void Multiply(int x)
{
    int i,j,m;
    for(i=3; i<=7; i=i+2)
    {
        for(j=1; j<=x; j++)
        {
            m=i*j;
            printf("%d*%d=%d\t",i,j,m);
        }
        printf("\n");
    }
}
